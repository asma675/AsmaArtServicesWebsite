{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)\\.(css|js|png|jpg|jpeg|gif|svg|webp|ico|txt|woff2?)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "dest": "/\\1"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}